<body>

	<?php
		function SeculoAno($ano) 
		{

		$seculo = ceil($ano/ 100);
		return $seculo;
		}

		$resultado = SeculoAno($ano = 100);
		echo "Seculo";
		echo $resultado; 


		$resultado = SeculoAno($ano = 200);
		echo "<br />Seculo ";
		echo $resultado;

	?>



